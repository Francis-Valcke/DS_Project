<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="288.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="logic.LoginController">
    <children>
        <Pane layoutX="23.0" layoutY="55.0" prefHeight="306.0" prefWidth="248.0">
            <children>
                <TextField fx:name="input_username" layoutY="47.0" prefHeight="31.0" prefWidth="248.0"
                           promptText="Username"/>
                <PasswordField fx:name="input_password" layoutY="99.0" prefHeight="31.0" prefWidth="248.0"
                               promptText="Password"/>
                <Button fx:name="button_signin" onAction="#login" layoutX="30.0" layoutY="177.0" prefHeight="31.0"
                        prefWidth="78.0" style="-fx-background-color: #257EFF;" text="Sign in" textFill="WHITE"/>
                <Button fx:name="button_register" onAction="#register" layoutX="145.0" layoutY="177.0" prefHeight="31.0"
                        prefWidth="78.0" style="-fx-background-color: #257EFF;" text="Register" textFill="WHITE"/>
            </children>
        </Pane>
        <Label alignment="CENTER" layoutX="-2.0" layoutY="14.0" prefHeight="51.0" prefWidth="292.0" text="Memory"
               textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="36.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
